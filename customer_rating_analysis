   "source": [
    "# 6.) Best product suggestion using popularity and collabrative-based filtering"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 58,
   "metadata": {},
   "outputs": [],
   "source": [
    "# List of suggested product by collabrative model filtering\n",
    "def show_me_best_products(products_list):\n",
    "    \n",
    "    # Extract product form the original list\n",
    "    sample1=sample2[sample2['productId'].isin(products_list)][['productId','Rating']]\n",
    "    \n",
    "    # count product wise total ratings\n",
    "    ds_ratings = sample1.groupby(by='productId')['Rating'].count()\n",
    "    \n",
    "    # Calculate average value of product rating \n",
    "    ds_mean_rating = pd.DataFrame(sample1.groupby('productId')['Rating'].mean())\n",
    "\n",
    "    # count number of product ratings\n",
    "    ds_mean_rating['Total'] = pd.DataFrame(sample1.groupby('productId')['Rating'].count())\n",
    "\n",
    "    # Count product wise ratings\n",
    "    suggested_products = pd.DataFrame(sample1.groupby('productId')['Rating'].count())\n",
    "\n",
    "    # Sorting descending order\n",
    "    suggested_products = suggested_products.sort_values('Rating', ascending=False)\n",
    "\n",
    "    # Display most popular products\n",
    "    suggested_products.head(10).plot(kind = \"bar\",color=\"pink\")\n",
    "    \n",
    "    print(suggested_products.head(10))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 23,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Customer Buying: 9790781369\n",
      "\n",
      "Suggestion to also buy: ['9790779798', '9790781369', 'B000052YMV', 'B00005NFBD', 'B000181TMY', 'B0009L5Z32', 'B000C1W39S', 'B000C1Z2D2', 'B000C1Z4S0', 'B000F3WCGM']\n"
     ]
    }
   ],
   "source": [
    "# Product ID as index of the product in dataset\n",
    "list1=suggest_me_collaborative(9)"
   ]
  },
