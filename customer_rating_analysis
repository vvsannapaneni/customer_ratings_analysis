    "\n",
    "# 5). Product buying suggestion based on collaborative filtering"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 15,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Select sample data of 20000 ratings\n",
    "sample2=ds.iloc[:20000,0:]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 16,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Total rows:  12669\n",
      "Total columns:  15450\n"
     ]
    }
   ],
   "source": [
    "# Create sparce matrix and fill up null value by zero\n",
    "ds_ratings = sample2.pivot_table(values='Rating', index='userId', columns='productId', fill_value=0)\n",
    "\n",
    "# now create matrix transpose\n",
    "ds_ratings = ds_ratings.T\n",
    "\n",
    "# Display total number of rows and column in dataset\n",
    "print('Total rows: ', ds_ratings.shape[0])\n",
    "print('Total columns: ', ds_ratings.shape[1])"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 17,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "(12669, 10)\n"
     ]
    }
   ],
   "source": [
    "# Now decompose above matrix\n",
    "SVD = TruncatedSVD(n_components=10)\n",
    "\n",
    "# Fitting the tranformation\n",
    "dec_matrix = SVD.fit_transform(ds_ratings)\n",
    "\n",
    "# display sample matrix\n",
    "print(dec_matrix.shape)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 18,
