     "output_type": "execute_result"
    }
   ],
   "source": [
    "crm"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "Now let's suggest 10 product based on selected item by he or she already bought"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 22,
   "metadata": {},
   "outputs": [],
   "source": [
    "\"\"\"\n",
    "Index : product index value in the list\n",
    "\"\"\"\n",
    "def suggest_me_collaborative(index):\n",
    "\n",
    "    # Now select one product from the list i.e. 80 (already bought by the customer)\n",
    "    productId=ds_ratings.index[index]\n",
    "    print('Customer Buying:',productId)\n",
    "\n",
    "    # extract related product\n",
    "    correlation_product = crm[index]\n",
    "\n",
    "    # select product correlation coefficient higher than .90\n",
    "    products_list = list(ds_ratings.index[correlation_product > 0.90])\n",
    "    \n",
    "    # Select 10 products\n",
    "    product_list=products_list[0:10]\n",
    "\n",
    "    # display list of 10 recommend products\n",
    "    print('\\nSuggestion to also buy:',product_list)\n",
    "    return product_list"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 89,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Customer Buying: B000052XZX\n",
      "\n",
      "Suggestion to also buy: ['B000052XZX', 'B000052Y25', 'B0000535UT', 'B0000UTUS8', 'B000142ZFS', 'B00015HBD8', 'B00017XZLI', 'B000181TMY', 'B000186X4I', 'B00022DP08']\n"
     ]
    }
   ],
   "source": [
    "# Product ID as index of the product in dataset\n",
    "list1=suggest_me_collaborative(20)"
   ]
  },
